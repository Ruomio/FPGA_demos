#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x555d73f09df0 .scope module, "testbench" "testbench" 2 4;
 .timescale -9 -9;
v0x555d73f1cb30_0 .var "clk", 0 0;
v0x555d73f1cc00_0 .net "led", 5 0, L_0x555d73f1ccd0;  1 drivers
S_0x555d73f09f80 .scope module, "u_top" "top" 2 23, 3 2 0, S_0x555d73f09df0;
 .timescale -9 -9;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 6 "led";
P_0x555d73f0a160 .param/l "WAIT_TIME" 0 3 8, +C4<00000000000000000000000000000101>;
L_0x555d73f1ccd0 .functor BUFZ 6, v0x555d73f1c900_0, C4<000000>, C4<000000>, C4<000000>;
v0x555d73ef80f0_0 .net "clk", 0 0, v0x555d73f1cb30_0;  1 drivers
v0x555d73f1c820_0 .var "clockCounter", 23 0;
v0x555d73f1c900_0 .var "cur_state", 5 0;
v0x555d73f1c9f0_0 .net "led", 5 0, L_0x555d73f1ccd0;  alias, 1 drivers
E_0x555d73ed1210 .event posedge, v0x555d73ef80f0_0;
    .scope S_0x555d73f09f80;
T_0 ;
    %pushi/vec4 0, 0, 24;
    %store/vec4 v0x555d73f1c820_0, 0, 24;
    %pushi/vec4 63, 0, 6;
    %store/vec4 v0x555d73f1c900_0, 0, 6;
    %end;
    .thread T_0;
    .scope S_0x555d73f09f80;
T_1 ;
    %wait E_0x555d73ed1210;
    %load/vec4 v0x555d73f1c820_0;
    %addi 1, 0, 24;
    %assign/vec4 v0x555d73f1c820_0, 0;
    %load/vec4 v0x555d73f1c820_0;
    %pad/u 32;
    %cmpi/e 4, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 24;
    %assign/vec4 v0x555d73f1c820_0, 0;
    %load/vec4 v0x555d73f1c900_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %assign/vec4 v0x555d73f1c900_0, 0;
    %load/vec4 v0x555d73f1c900_0;
    %cmpi/e 0, 0, 6;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 63, 0, 6;
    %assign/vec4 v0x555d73f1c900_0, 0;
T_1.2 ;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x555d73f09df0;
T_2 ;
    %delay 1, 0;
    %load/vec4 v0x555d73f1cb30_0;
    %inv;
    %store/vec4 v0x555d73f1cb30_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x555d73f09df0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555d73f1cb30_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x555d73f09df0;
T_4 ;
    %vpi_call 2 17 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 18 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x555d73f09df0 {0 0 0};
    %delay 6000, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "top_tb.v";
    "top.v";
